# =================================
# APPLICATION SETTINGS
# =================================
NODE_ENV=development
PORT=3001
APP_NAME="GNS3 Network Automation Platform"

# =================================
# GNS3 INTEGRATION
# =================================
GNS3_SERVER_URL=http://127.0.0.1:3080
GNS3_USERNAME=admin
GNS3_PASSWORD=admin

# =================================
# DEFAULT DEVICE CREDENTIALS
# =================================
DEVICE_USERNAME=admin
DEVICE_PASSWORD=admin
SSH_TIMEOUT=10000

# =================================
# AUTHENTICATION
# =================================
JWT_SECRET=your-super-secret-jwt-key-minimum-32-characters

# =================================
# CORS SETTINGS
# =================================
CORS_ORIGIN=http://localhost:5173

# =================================
# DEVICE MANAGEMENT
# =================================
DEVICE_POLL_INTERVAL=300
ROUTER_IP=192.168.1.10
SWITCH_IP=192.168.1.11
FIREWALL_IP=192.168.1.12